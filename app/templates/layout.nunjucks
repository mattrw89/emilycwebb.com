<!doctype html>
<html class="no-js" lang="">
<head>
  <title><$ title $></title>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta http-equiv="x-dns-prefetch-control" content="on">
  <link rel="dns-prefetch" href="//cdn.jsdelivr.net">

  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="author" content="<$ graph.author $>">
  <meta name="description" content="<$ graph.description $>">
  <link rel="canonical" href="<$ graph.url $>">
  <meta property="og:locale" content="en_US">
  <meta property="og:type" content="website">
  <meta property="og:title" content="<$ graph.title $>">
  <meta property="og:description" content="<$ graph.description $>">
  <meta property="og:url" content="<$ graph.url $>">
  <meta property="og:site_name" content="Emily Webb">
  <meta property="og:image" content="<$ graph.image $>">
  <!--<meta name="twitter:card" content="summary_large_image">-->
  <meta name="twitter:description" content="<$ graph.description $>">
  <meta name="twitter:title" content="<$ graph.title $>">
  <meta name="twitter:site" content="<$ graph.twitterHandle $>">
  <meta name="twitter:domain" content="<$ graph.twitterDomain $>">
  <meta name="twitter:image:src" content="<$ graph.twitterImage $>">
  <meta name="twitter:creator" content="<$ graph.twitterHandle $>">

  <!-- Fav and touch icons -->
  <link rel="shortcut icon" href="<$ faviconUrl $>"/>

  <!-- Standard iPhone -->
  <!--<link rel="apple-touch-icon" sizes="57x57" href="//d327l0dy67qzb.cloudfront.net/static_assets/icons/iPhone@2x.png"/>-->
  <!-- Retina iPhone -->
  <!--<link rel="apple-touch-icon" sizes="114x114" href="//d327l0dy67qzb.cloudfront.net/static_assets/icons/iPhone@2x.png"/>-->
  <!-- Standard iPad -->
  <!--<link rel="apple-touch-icon" sizes="72x72" href="//d327l0dy67qzb.cloudfront.net/static_assets/icons/iPad@2x.png"/>-->
  <!-- Retina iPad -->
  <!--<link rel="apple-touch-icon" sizes="144x144" href=//d327l0dy67qzb.cloudfront.net/static_assets/icons/iPad@2x.png"/>-->

  <!-- compiled css load -->
  <link rel="stylesheet" href="styles/styles.css">

  <% block additional_head %><% endblock %>
</head>
<body class="<$ currentPage $>">
  <% block content %><% endblock %>

  <% block footer_container %>
  <% include "partials/_footer.nunjucks" %>
  <% endblock %>

  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();
      i.initAnalytics=function(){a=s.createElement(o),m=s.getElementsByTagName(o)[0];
        a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      }})(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    ga('create', 'UA-75130183-1', 'auto');
    ga('send','pageview');

    setTimeout(function() {
      initAnalytics();
    }, 3000.0)
  </script>


  <script src="//cdn.jsdelivr.net/g/jquery@2.1.4(jquery.min.js),jquery.lazyload@1.9.3(jquery.lazyload.min.js)"></script>

  <!-- load compiled JS -->
  <script src="scripts/app.js"></script>

  <script type="text/javascript">
    $(document).foundation();
  </script>

</body>
</html>
